          },
          "age": {
            "$id": "#/properties/accordion2/properties/age",
            "type": "integer",
            "title": "The Age Schema",
            "default": 31
          }
        }
      },
      "accordion3": {
        "$id": "#/properties/accordion3",
        "type": "object",
